<h1>Listing bases</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Name</th>
      <th>Tags</th>
      <th>Date</th>
      <th>Examples</th>
      <th>Knowledge</th>
      <th>Instructions</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @bases.each do |basis| %>
      <tr>
        <td><%= basis.id %></td>
        <td><%= basis.name %></td>
        <td><%= basis.tags %></td>
        <td><%= basis.date %></td>
        <td><%= basis.examples %></td>
        <td><%= basis.knowledge %></td>
        <td><%= basis.instructions %></td>
        <td><%= link_to 'Show', basis %></td>
        <td><%= link_to 'Edit', edit_basis_path(basis) %></td>
        <td><%= link_to 'Destroy', basis, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Base', new_basis_path %>
